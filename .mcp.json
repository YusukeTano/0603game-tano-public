{
  "mcpServers": {
    "aws-documentation-mcp-server": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--python",
        "3.12",
        "--native-tls",
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_DOCUMENTATION_PARTITION": "aws",
        "SSL_CERT_FILE": "/Users/tano/.prismaaccess/cert_IRET-SSL-TrsutCA.crt",
        "NODE_EXTRA_CA_CERTS": "/Users/tano/.prismaaccess/cert_IRET-SSL-TrsutCA.crt",
        "REQUESTS_CA_BUNDLE": "/Users/tano/.prismaaccess/cert_IRET-SSL-TrsutCA.crt"
      }
    },
    "aws-knowledge-mcp-server": {
      "type": "http",
      "url": "https://knowledge-mcp.global.api.aws"
    },
    "aws-terraform-mcp": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "awslabs.terraform-mcp-server@latest"
      ],
      "env": {}
    }
  }
}